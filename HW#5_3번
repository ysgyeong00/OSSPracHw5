#include <iostream>
using namespace std;

int main() {
	string name;
	int stdnum;

	cout << "이름을 입력하세요 : ";
	cin >> name;
	cout << "학번을 입력하세요 : ";
	cin >> stdnum;

	cout << "\n<출력>\n" << "이름 : ";
	for (int i = 0; i < 6; i++) {
		cout << name[i];
	}
	cout << "\n학번 : " << stdnum << "\n";

	return 0;
}
